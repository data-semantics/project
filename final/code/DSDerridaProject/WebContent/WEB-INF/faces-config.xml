<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>com.tutorial.LoginBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>orderBean</managed-bean-name>
		<managed-bean-class>com.tutorial.OrderBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>editorBean</managed-bean-name>
		<managed-bean-class>com.tutorial.EditorBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>openSearchBean</managed-bean-name>
		<managed-bean-class>com.tutorial.OpenSearchBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/welcome.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>welcome.xhtml</display-name>
		<from-view-id>/welcome.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>backlogin</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>result</from-outcome>
			<to-view-id>/customresult.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>customresult.xhtml</display-name>
		<from-view-id>/customresult.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>backlogin</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>gosparql</from-outcome>
			<to-view-id>/sparqlEditor.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>sparqlEditor.xhtml</display-name>
		<from-view-id>/sparqlEditor.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>backlogin</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>sparqlEditor.xhtml</display-name>
		<from-view-id>/sparqlEditor.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>queryeditor</from-outcome>
			<to-view-id>/queryresults.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>queryresults.xhtml</display-name>
		<from-view-id>/queryresults.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>backtoeditor</from-outcome>
			<to-view-id>/sparqlEditor.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>bookPage</from-outcome>
			<to-view-id>/booktemplate.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>booktemplate.xhtml</display-name>
		<from-view-id>/booktemplate.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>backlogin</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>gallery</from-outcome>
			<to-view-id>gallery.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>gallery.xhtml</display-name>
		<from-view-id>gallery.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>gotologin</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>bookLink</from-outcome>
			<to-view-id>/bookLinks.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>bookLinks.xhtml</display-name>
		<from-view-id>/bookLinks.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>backlogin</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	

</faces-config>
